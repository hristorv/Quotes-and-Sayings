<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >


    <com.afollestad.materialdialogs.prefs.MaterialListPreference
        android:defaultValue="@string/pref_interval_default"
        android:dialogTitle="@string/pref_interval"
        android:entries="@array/pref_interval_entry"
        android:entryValues="@array/pref_interval_value"
        android:key="pref_widget_interval"
